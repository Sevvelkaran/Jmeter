Random random = new Random();
String place1 = &quot;&quot;;
String place2 = &quot;&quot;;

do {
    place1 = arr1[random.nextInt(arr1.length)];
    place2 = arr2[random.nextInt(arr2.length)];
} while (place1.equals(place2));

vars.put(&quot;place1&quot;, place1);
vars.put(&quot;place2&quot;, place2);


log.info(&quot;Place 1: &quot; + place1);
log.info(&quot;Place 2: &quot; + place2);
