          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.jsoup.Jsoup
import org.jsoup.nodes.Document
import org.jsoup.select.Elements
import java.util.Random

// Get HTML from previous sampler response
String html = prev.getResponseDataAsString()

// Parse HTML with Jsoup
Document doc = Jsoup.parse(html)

// Select all table rows inside tbody
Elements rows = doc.select(&quot;table.table tbody tr&quot;)

def flightList = []

rows.each { row -&gt;
    Elements cols = row.select(&quot;td&quot;)

    // Expecting at least 6 columns (first is button)
    if (cols.size() &gt;= 6) {
        try {
            String flightNo = cols[1].text().trim()
            String airline = cols[2].text().trim()
            String departure = cols[3].text().trim()
            String arrival = cols[4].text().trim()
            String priceText = cols[5].text().replace(&apos;$&apos;, &apos;&apos;).trim()
            float price = Float.parseFloat(priceText)

            flightList &lt;&lt; [
                flightNo : flightNo,
                airline  : airline,
                departure: departure,
                arrival  : arrival,
                price    : price
            ]
        } catch (Exception e) {
            log.warn(&quot;Skipping row due to parse error: &quot; + e.message)
        }
    }
}

if (!flightList.isEmpty()) {
    def random = new Random()
    def selectedFlight = flightList[random.nextInt(flightList.size())]

    vars.put(&apos;selectedFlightNumber&apos;, selectedFlight.flightNo)
    vars.put(&apos;selectedAirline&apos;, selectedFlight.airline)
    vars.put(&apos;selectedDeparture&apos;, selectedFlight.departure)
    vars.put(&apos;selectedArrival&apos;, selectedFlight.arrival)
    vars.put(&apos;selectedPrice&apos;, selectedFlight.price.toString())
} else {
    log.error(&quot;❌ No valid flights found in response!&quot;)
}
</stringProp>
          </JSR223PreProcessor>
