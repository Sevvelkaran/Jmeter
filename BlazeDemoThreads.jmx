            <stringProp name="script">// Dropdown list 1
def list1 = [&apos;Paris&apos;, &apos;Philadelphia&apos;, &apos;Boston&apos;, &apos;Portland&apos;, &apos;San Diego&apos;, &apos;Mexico City&apos;, &apos;São Paolo&apos;]

// Dropdown list 2
def list2 = [&apos;Buenos Aires&apos;, &apos;Rome&apos;, &apos;London&apos;, &apos;Berlin&apos;, &apos;New York&apos;, &apos;Dublin&apos;, &apos;Cairo&apos;]

def random = new Random()
def place1
def place2

// Ensure different values using do-while
do {
    place1 = list1[random.nextInt(list1.size())]
    place2 = list2[random.nextInt(list2.size())]
} while (place1 == place2)

// Store in JMeter variables
vars.put(&apos;place1&apos;, place1)
vars.put(&apos;place2&apos;, place2)</stringProp>
